<div class="col">
    <figure class="figure">
            <img src="https://getuikit.com/v2/docs/images/placeholder_600x400.svg" class="figure-img img-fluid rounded" "><br>
            <%= imagen.descripcion %>
            <%= link_to 'Editar', edit_imagen_path(imagen) %>
            <%= link_to 'Eliminar', imagen, method: :delete, data: { confirm: 'Esta seguro que desea eliminar la imagen?'} %>
            <figcaption class="figure-caption"></figcaption>
    </figure>
</div>